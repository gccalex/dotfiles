#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls -la --color=auto'
alias grep='grep --color=auto'
PS1='[\u@\h \W]\$ '

alias c='clear'
alias n='neofetch'

#git 

alias g=’git’
alias st=’git status’
alias com=’git commit -m’
alias clone=’git clone’
alias sth=’git stash’
alias lg=’git log’
alias u=’git add -u’
alias all=’git add .’
